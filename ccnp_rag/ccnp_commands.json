{
  "commands": [
    {
      "command": "show ip route",
      "description": "Displays the IP routing table with all known routes",
      "category": "routing",
      "protocol": "all",
      "mode": "privileged",
      "examples": [
        "show ip route",
        "show ip route 192.168.1.0",
        "show ip route ospf",
        "show ip route bgp"
      ],
      "parameters": [
        {
          "name": "network",
          "description": "Specific network to display",
          "optional": true
        },
        {
          "name": "protocol",
          "description": "Filter by routing protocol (ospf, bgp, isis, static)",
          "optional": true
        }
      ]
    },
    {
      "command": "configure terminal",
      "description": "Enters global configuration mode",
      "category": "configuration",
      "protocol": "all",
      "mode": "privileged",
      "examples": [
        "configure terminal",
        "conf t"
      ],
      "parameters": []
    },
    {
      "command": "router bgp",
      "description": "Configures BGP routing process with specified AS number",
      "category": "routing",
      "protocol": "bgp",
      "mode": "global",
      "examples": [
        "router bgp 65001",
        "router bgp 100"
      ],
      "parameters": [
        {
          "name": "as_number",
          "description": "Autonomous System number",
          "optional": false
        }
      ]
    },
    {
      "command": "network",
      "description": "Advertises network in BGP",
      "category": "routing",
      "protocol": "bgp",
      "mode": "router_bgp",
      "examples": [
        "network 192.168.1.0",
        "network 10.0.0.0 mask 255.255.255.0",
        "network 172.16.0.0/16"
      ],
      "parameters": [
        {
          "name": "network",
          "description": "Network to advertise",
          "optional": false
        },
        {
          "name": "mask",
          "description": "Subnet mask (optional)",
          "optional": true
        }
      ]
    },
    {
      "command": "neighbor",
      "description": "Configures BGP neighbor",
      "category": "routing",
      "protocol": "bgp",
      "mode": "router_bgp",
      "examples": [
        "neighbor 192.168.1.1 remote-as 65002",
        "neighbor 10.0.0.1 remote-as 100",
        "neighbor 172.16.1.1 remote-as 200"
      ],
      "parameters": [
        {
          "name": "ip_address",
          "description": "Neighbor IP address",
          "optional": false
        },
        {
          "name": "remote-as",
          "description": "Remote AS number",
          "optional": false
        }
      ]
    },
    {
      "command": "router ospf",
      "description": "Configures OSPF routing process",
      "category": "routing",
      "protocol": "ospf",
      "mode": "global",
      "examples": [
        "router ospf 1",
        "router ospf 100"
      ],
      "parameters": [
        {
          "name": "process_id",
          "description": "OSPF process ID",
          "optional": false
        }
      ]
    },
    {
      "command": "network",
      "description": "Configures OSPF network",
      "category": "routing",
      "protocol": "ospf",
      "mode": "router_ospf",
      "examples": [
        "network 10.0.0.0 0.255.255.255 area 0",
        "network 192.168.1.0 0.0.0.255 area 1",
        "network 172.16.0.0 0.0.255.255 area 0"
      ],
      "parameters": [
        {
          "name": "network",
          "description": "Network address",
          "optional": false
        },
        {
          "name": "wildcard_mask",
          "description": "Wildcard mask",
          "optional": false
        },
        {
          "name": "area",
          "description": "OSPF area",
          "optional": false
        }
      ]
    },
    {
      "command": "show ip ospf neighbor",
      "description": "Displays OSPF neighbor information",
      "category": "routing",
      "protocol": "ospf",
      "mode": "privileged",
      "examples": [
        "show ip ospf neighbor",
        "show ip ospf neighbor detail"
      ],
      "parameters": [
        {
          "name": "detail",
          "description": "Show detailed neighbor information",
          "optional": true
        }
      ]
    },
    {
      "command": "router isis",
      "description": "Configures IS-IS routing process",
      "category": "routing",
      "protocol": "isis",
      "mode": "global",
      "examples": [
        "router isis",
        "router isis 1"
      ],
      "parameters": [
        {
          "name": "tag",
          "description": "IS-IS process tag (optional)",
          "optional": true
        }
      ]
    },
    {
      "command": "net",
      "description": "Configures IS-IS NET ID",
      "category": "routing",
      "protocol": "isis",
      "mode": "router_isis",
      "examples": [
        "net 49.0001.1921.6800.1001.00",
        "net 49.0001.1921.6800.1002.00"
      ],
      "parameters": [
        {
          "name": "net_id",
          "description": "IS-IS NET ID",
          "optional": false
        }
      ]
    },
    {
      "command": "interface",
      "description": "Enters interface configuration mode",
      "category": "interface",
      "protocol": "all",
      "mode": "global",
      "examples": [
        "interface GigabitEthernet0/0",
        "interface FastEthernet0/1",
        "interface Serial0/0/0"
      ],
      "parameters": [
        {
          "name": "interface",
          "description": "Interface name and number",
          "optional": false
        }
      ]
    },
    {
      "command": "ip address",
      "description": "Configures IP address on interface",
      "category": "interface",
      "protocol": "all",
      "mode": "interface",
      "examples": [
        "ip address 192.168.1.1 255.255.255.0",
        "ip address 10.0.0.1 255.255.255.0",
        "ip address 172.16.1.1 255.255.0.0"
      ],
      "parameters": [
        {
          "name": "ip_address",
          "description": "IP address",
          "optional": false
        },
        {
          "name": "subnet_mask",
          "description": "Subnet mask",
          "optional": false
        }
      ]
    },
    {
      "command": "no shutdown",
      "description": "Enables interface",
      "category": "interface",
      "protocol": "all",
      "mode": "interface",
      "examples": [
        "no shutdown"
      ],
      "parameters": []
    },
    {
      "command": "show interfaces",
      "description": "Displays interface status and configuration",
      "category": "interface",
      "protocol": "all",
      "mode": "privileged",
      "examples": [
        "show interfaces",
        "show interfaces GigabitEthernet0/0",
        "show interfaces status"
      ],
      "parameters": [
        {
          "name": "interface",
          "description": "Specific interface to display",
          "optional": true
        },
        {
          "name": "status",
          "description": "Show interface status summary",
          "optional": true
        }
      ]
    },
    {
      "command": "show ip bgp",
      "description": "Displays BGP routing table",
      "category": "routing",
      "protocol": "bgp",
      "mode": "privileged",
      "examples": [
        "show ip bgp",
        "show ip bgp summary",
        "show ip bgp neighbors"
      ],
      "parameters": [
        {
          "name": "summary",
          "description": "Show BGP summary",
          "optional": true
        },
        {
          "name": "neighbors",
          "description": "Show BGP neighbors",
          "optional": true
        }
      ]
    },
    {
      "command": "show ip ospf",
      "description": "Displays OSPF information",
      "category": "routing",
      "protocol": "ospf",
      "mode": "privileged",
      "examples": [
        "show ip ospf",
        "show ip ospf database",
        "show ip ospf interface"
      ],
      "parameters": [
        {
          "name": "database",
          "description": "Show OSPF database",
          "optional": true
        },
        {
          "name": "interface",
          "description": "Show OSPF interface information",
          "optional": true
        }
      ]
    },
    {
      "command": "show ip isis",
      "description": "Displays IS-IS information",
      "category": "routing",
      "protocol": "isis",
      "mode": "privileged",
      "examples": [
        "show ip isis",
        "show ip isis neighbors",
        "show ip isis database"
      ],
      "parameters": [
        {
          "name": "neighbors",
          "description": "Show IS-IS neighbors",
          "optional": true
        },
        {
          "name": "database",
          "description": "Show IS-IS database",
          "optional": true
        }
      ]
    },
    {
      "command": "ping",
      "description": "Tests connectivity to destination",
      "category": "troubleshooting",
      "protocol": "all",
      "mode": "privileged",
      "examples": [
        "ping 192.168.1.1",
        "ping 10.0.0.1",
        "ping google.com"
      ],
      "parameters": [
        {
          "name": "destination",
          "description": "Destination IP address or hostname",
          "optional": false
        }
      ]
    },
    {
      "command": "traceroute",
      "description": "Traces route to destination",
      "category": "troubleshooting",
      "protocol": "all",
      "mode": "privileged",
      "examples": [
        "traceroute 192.168.1.1",
        "traceroute 10.0.0.1",
        "traceroute google.com"
      ],
      "parameters": [
        {
          "name": "destination",
          "description": "Destination IP address or hostname",
          "optional": false
        }
      ]
    },
    {
      "command": "show running-config",
      "description": "Displays current running configuration",
      "category": "configuration",
      "protocol": "all",
      "mode": "privileged",
      "examples": [
        "show running-config",
        "show run"
      ],
      "parameters": []
    },
    {
      "command": "copy running-config startup-config",
      "description": "Saves current configuration to startup configuration",
      "category": "configuration",
      "protocol": "all",
      "mode": "privileged",
      "examples": [
        "copy running-config startup-config",
        "copy run start"
      ],
      "parameters": []
    }
  ]
}
